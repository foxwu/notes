set nocompatible
set laststatus=2
set guifontset=8x16,kc15f,-*-16-*-big5-0
set cindent
set encoding=utf-8
set hls
set sw=2
set backspace=2
set paste
filetype on
filetype indent on
filetype plugin on
highlight Comment ctermfg=darkcyan
highlight Search term=reverse ctermbg=4 ctermfg=7
set expandtab
set softtabstop=4
set ruler
syntax on
set background=dark
